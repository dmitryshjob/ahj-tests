{
  "name": "ahj-tests",
  "version": "1.0.1",
  "description": "three tasks about UNIT-testing, Puppeteer and JSDOM",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "test": "jest --testPathPattern=./src/ --env=jsdom",
    "coverage": "jest --testPathPattern=./src/ --coverage",
    "e2e": "jest --clearCache && jest --testPathPattern=./e2e/ --runInBand --detectOpenHandles",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist",
    "ci": "start-server-and-test start :9000 e2e",
    "fix": "eslint . --fix"
  },
  "keywords": [
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "jsdom": "^22.1.0",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.35",
    "postcss-loader": "^7.3.4",
    "postcss-preset-env": "^8.5.1",
    "puppeteer": "^21.11.0",
    "sass": "^1.71.1",
    "sass-loader": "^13.3.3",
    "start-server-and-test": "^2.0.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "core-js": "^3.36.0"
  }
}
